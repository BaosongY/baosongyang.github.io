---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Adam Lopez</title>
 <link href="http://alopez.github.com/atom.xml" rel="self"/>
 <link href="http://alopez.github.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://alopez.github.com/</id>
 <author>
   <name>Adam Lopez</name>
   <email>alopez@inf.ed.ac.uk</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{% if post.layout == 'paper' %}Paper: {% endif %}{{ post.title }}</title>
   <link href="{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://http://alopez.github.com{{ post.id }}</id>
   <content type="html">{{ post.abstract }}</content>
 </entry>
 {% endfor %}
 
</feed>
